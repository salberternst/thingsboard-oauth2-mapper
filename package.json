    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "depcheck": "^1.4.5",
    "prettier": "^3.0.3",
    "standard": "^17.1.0"
  }
}
